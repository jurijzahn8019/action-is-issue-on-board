{
  "name": "@jurijzahn8019/action-is-issue-on-board",
  "version": "0.0.61",
  "description": "Checks whether an issue or pull is already added to the project board",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && for S in rollup; do npm run build:$S; done",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.prod.json -d --outDir ./dist --emitDeclarationOnly",
    "build:tsc": "tsc --noEmit",
    "build:watch": "tsc -w",
    "clean": "rm -rf ./dist ./tmp",
    "lint": "eslint --fix src/**/*.ts",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run build && git add dist/*",
    "prerel": "standard-version --skip.changelog -p $(git branch --show-current | sed 's/main/rc/g' | sed 's/[_\\/]/-/g')",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin main",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/action-is-issue-on-board.git"
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/action-is-issue-on-board/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/action-is-issue-on-board#readme",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "@octokit/types": "^6.34.0",
    "debug": "^4.3.4",
    "encoding": "^0.1.13",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "8.3.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.35",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.5",
    "eslint-plugin-prettier": "^4.0.0",
    "git-branch": "^2.0.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.0",
    "typescript": "4.7.2"
  }
}
